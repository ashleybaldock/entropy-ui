{
  "name": "entropy-ui",
  "version": "0.5.0",
  "description": "A chaotic React component library",
  "main": "./lib/entropy-ui.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack --watch",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook --static-dir public --output-dir ./storybook --config-dir .storybook"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashleybaldock/entropy-ui.git"
  },
  "keywords": [],
  "author": "Ashley Baldock (ashley@baldock.me)",
  "license": "BSD-4-Clause",
  "bugs": {
    "url": "https://github.com/ashleybaldock/entropy-ui/issues"
  },
  "homepage": "https://entropy-ui.morepossibility.com",
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": 1
    },
    "overrides": [
      {
        "files": [
          "src/**/*.stories.js"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "typings": "./index.d.ts",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-controls": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addons": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.21.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
