{
  "name": "entropy-ui",
  "version": "0.4.0",
  "description": "A chaotic React component library",
  "main": "./lib/entropy-ui.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack --watch",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook --static-dir public --output-dir ./storybook --config-dir .storybook"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashleybaldock/entropy-ui.git"
  },
  "keywords": [],
  "author": "Ashley Baldock (ashley@baldock.me)",
  "license": "BSD-4-Clause",
  "bugs": {
    "url": "https://github.com/ashleybaldock/entropy-ui/issues"
  },
  "homepage": "https://entropy-ui.morepossibility.com",
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": 1
    },
    "overrides": [
      {
        "files": [
          "src/**/*.stories.js"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "typings": "./index.d.ts",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@storybook/addon-actions": "^6.2.x",
    "@storybook/addon-knobs": "^6.2.x",
    "@storybook/addon-links": "^6.2.x",
    "@storybook/addons": "^6.2.x",
    "@storybook/react": "^6.4.9",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.1",
    "eslint": "^8.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "style-loader": "^2.0.0",
    "webpack": "^4",
    "webpack-cli": "^4.9.1"
  }
}
